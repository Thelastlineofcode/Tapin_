<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 300 300" width="300" height="300">
  <!-- Square logo version for favicons and app icons -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4a90e2;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#6366f1;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Background -->
  <rect width="300" height="300" rx="40" fill="url(#bgGradient)"/>

  <!-- Large T letter -->
  <g transform="translate(150, 120)">
    <path d="M -50,-40 L 50,-40 L 50,-20 L 15,-20 L 15,60 L -15,60 L -15,-20 L -50,-20 Z"
          fill="#ffffff" stroke="none"/>
  </g>

  <!-- Connection circles -->
  <circle cx="220" cy="100" r="20" fill="none" stroke="#ffffff" stroke-width="4"/>
  <circle cx="220" cy="100" r="12" fill="#ffffff"/>

  <circle cx="80" cy="200" r="16" fill="none" stroke="#ffffff" stroke-width="3" opacity="0.8"/>
  <circle cx="80" cy="200" r="8" fill="#ffffff" opacity="0.8"/>

  <!-- Connecting line -->
  <path d="M 150,180 Q 180,160 200,120" stroke="#ffffff" stroke-width="3"
        stroke-linecap="round" fill="none" opacity="0.6" stroke-dasharray="5,5"/>

  <!-- Bottom text -->
  <text x="150" y="260" font-family="Arial, sans-serif" font-size="24"
        font-weight="bold" fill="#ffffff" text-anchor="middle">TapIn</text>
</svg>
