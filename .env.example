# Flask Configuration
SECRET_KEY=your-secret-key-here
JWT_SECRET_KEY=your-jwt-secret-key-here
SECURITY_PASSWORD_SALT=your-security-salt-here

# Database Configuration
SQLALCHEMY_DATABASE_URI=sqlite:///tapin.db

# Email Configuration (SMTP)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-email-password-here
SMTP_USE_TLS=true

# Event API Keys
# Get your Eventbrite API key from: https://www.eventbrite.com/platform/api
EVENTBRITE_API_KEY=your-eventbrite-api-key-here

# Get your Ticketmaster API key from: https://developer.ticketmaster.com/
TICKETMASTER_API_KEY=your-ticketmaster-api-key-here

# Get your Meetup API key from: https://www.meetup.com/api/
MEETUP_API_KEY=your-meetup-api-key-here

# Get your PredictHQ API key from: https://www.predicthq.com/
PREDICTHQ_API_KEY=your-predicthq-api-key-here
